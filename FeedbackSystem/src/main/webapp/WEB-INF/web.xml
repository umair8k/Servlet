<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Feed_Back_System</display-name>
  <welcome-file-list>
  <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
   <servlet>

        <servlet-name>RegSer</servlet-name>
        <servlet-class>RegSer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginSer</servlet-name>
        <servlet-class>LoginSer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FeedSer</servlet-name>
        <servlet-class>FeedSer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminSer</servlet-name>
        <servlet-class>AdminSer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegSer</servlet-name>
        <url-pattern>/RegSer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginSer</servlet-name>
        <url-pattern>/LoginSer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FeedSer</servlet-name>
        <url-pattern>/FeedSer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminSer</servlet-name>
        <url-pattern>/AdminSer</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>